{
  "name": "@vendin/tenant-instance",
  "version": "0.0.1",
  "description": "MedusaJS Tenant Instance Template",
  "author": "",
  "license": "MIT",
  "keywords": [
    "medusa",
    "medusa-store",
    "ecommerce",
    "headless",
    "typescript"
  ],
  "scripts": {
    "build": "medusa build",
    "start": "medusa start",
    "dev": "medusa develop",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest",
    "db:migrate": "TS_NODE_PROJECT=tsconfig.json medusa db:migrate",
    "db:seed": "TS_NODE_PROJECT=tsconfig.json medusa exec ./src/scripts/seed.ts"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "^2.13.0",
    "@medusajs/framework": "^2.13.0",
    "@medusajs/medusa": "^2.13.0",
    "@vendin/analytics": "workspace:*",
    "pg": "^8.17.2"
  },
  "devDependencies": {
    "@medusajs/cli": "^2.13.0",
    "@medusajs/test-utils": "^2.13.0",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^4.0.18",
    "ajv": "^8.17.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
