{
  "name": "@vendin/utils",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "bun": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./logger": {
      "import": "./dist/logger.js",
      "bun": "./src/logger.ts",
      "default": "./dist/logger.js"
    }
  },
  "scripts": {
    "build": "bun x tsc",
    "lint": "bun x eslint .",
    "lint:fix": "bun x eslint . --fix",
    "typecheck": "bun x tsc --noEmit"
  },
  "dependencies": {
    "pino": "^10.1.0"
  },
  "devDependencies": {
    "@vendin/config": "workspace:*",
    "@types/bun": "^1.1.10",
    "eslint": "^9.37.0",
    "pino-pretty": "^13.1.2",
    "typescript": "^5.9.3"
  }
}
