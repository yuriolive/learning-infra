{
  "name": "@vendin/utils",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./logger": {
      "import": "./dist/logger.js",
      "default": "./src/logger.ts"
    },
    "./logger-cloudflare": {
      "import": "./dist/logger-cloudflare.js",
      "default": "./src/logger-cloudflare.ts"
    },
    "./logger-cloudflare-factory": {
      "import": "./dist/logger-cloudflare-factory.js",
      "default": "./src/logger-cloudflare-factory.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "pino": "^10.1.0",
    "validator": "^13.15.26",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/validator": "^13.15.10",
    "@vendin/config": "workspace:*",
    "eslint": "^9.37.0",
    "pino-pretty": "^13.1.2",
    "typescript": "^5.9.3"
  }
}
